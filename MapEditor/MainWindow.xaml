<Window x:Class="MapEditor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MapEditor"
        xmlns:vm="clr-namespace:MapEditor.ViewModels"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Window.DataContext>
        <vm:ViewModel/>
    </Window.DataContext>
    <Window.Resources>
        <local:SampleConverter x:Key="SampleConverter" />
    </Window.Resources>
    <Grid>
        <Button Content="Yes No Dialog" Command="{Binding YesNoCommand}" HorizontalAlignment="Left" Margin="123,91,0,0" VerticalAlignment="Top" />
        <Button Content="Alert Dialog" Command="{Binding AlertCommand}" HorizontalAlignment="Left" Margin="309,109,0,0" VerticalAlignment="Top" />
    </Grid>
</Window>
